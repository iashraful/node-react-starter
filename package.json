{
  "name": "node-react-starter",
  "version": "1.0.0",
  "description": "A basic nodejs and react application",
  "main": "index.js",
  "author": "Ashraful Islam",
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn run start",
    "server": "nodemon --exec babel-node index.js",
    "dev": "concurrently -r -k \"yarn run server\" \"yarn run client\"",
    "start": "node index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "mongoose": "^5.7.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  }
}
